<Window x:Class="mgmtapplauncher2.ProtocolNamePopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Podaj nazwę" Height="150" Width="200" Icon="Resources/mgmtapplauncher2.ico" ResizeMode="NoResize"
        FocusManager.FocusedElement="{Binding ElementName=TBProtocolName}">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Content="Podaj nazwę protokołu:" Grid.ColumnSpan="2" Margin="5" VerticalAlignment="Center"/>
        <TextBox x:Name="TBProtocolName" Grid.ColumnSpan="2" Grid.Row="1" Margin="10" VerticalContentAlignment="Center"/>
        <Button x:Name="BNameOK" Click="BNameOK_Click" Content="OK" Grid.Row="2" IsDefault="True" Margin="10,8"/>
        <Button x:Name="BNameCancel" Click="BNameCancel_Click" Content="Anuluj" Grid.Column="1" Grid.Row="2" IsCancel="True" Margin="10,8"/>
    </Grid>
</Window>
